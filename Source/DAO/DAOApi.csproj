<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <Import Project="..\..\.build\Common.props" />
  <PropertyGroup>
    <TargetFrameworks>net40;net45;netcoreapp3.0</TargetFrameworks>
    <AssemblyTitle>DAO</AssemblyTitle>
    <Description>NetOffice wrapper for Microsoft DAO Object Library.</Description>
    <RootNamespace>NetOffice.DAOApi</RootNamespace>
    <IsPackable>false</IsPackable>
  </PropertyGroup>
  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>DAOApi_v4.0.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\NetOffice\NetOffice.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="DAOApi_v4.0.snk" />
  </ItemGroup>
</Project>